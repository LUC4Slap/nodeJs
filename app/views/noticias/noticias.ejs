<!DOCTYPE html>
<html lang="pt-br">
  <head>
    <meta charset="utf-8" />
    <title>Notícias</title>
  </head>
  <body>
    <h1>Notícias</h1>

    <table>
      <tr>
        <td>ID</td>
        <td>Titulo</td>
      </tr>

      <% for(var i=0; i < noticias.length; i++) { %> <% if (i !== undefined) {
      %>
      <tr>
        <td><%= noticias[i].id_noticia %></td>
        <td><%= noticias[i].titulo %></td>
      </tr>
      <% } %> <% } %>
      <tr>
        <td>Não ha noticias!!!!</td>
      </tr>
    </table>
  </body>
</html>
